            "modifiers": {
              "mandatory": ["right_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": ["right_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "end"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "slash",
            "modifiers": {
              "mandatory": ["right_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ]
        }
      ]
    }
  ]
}
