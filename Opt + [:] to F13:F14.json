{
  "title": "Opt + [/] to F13/F14",
  "rules": [
    {
      "description": "Opt + [/] to F13/F14",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["left_option"]
            }
          },
          "to": [
            {
              "key_code": "f13"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": ["left_option"]
            }
          },
          "to": [
            {
              "key_code": "f14"
            }
          ]
        }
      ]
    },
    {
      "description": "Opt + [/] to F13/F14 Right",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["right_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "f13"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": ["right_option"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "f14"
            }
          ]
        }
      ]
    }
  ]
}
